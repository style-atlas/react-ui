{
  "name": "@styleatlas/react-ui",
  "version": "0.1.2",
  "description": "Style Atlas ReactJS UI Components",
  "main": "lib/index.js",
  "author": {
    "email": "hello@styleatlas.com",
    "name": "Style Atlas",
    "url": "http://styleatlas.co"
  },
  "homepage": "https://github.com/style-atlas/react-ui",
  "bugs": "https://github.com/style-atlas/react-ui/issues",
  "license": "MIT",
  "private": false,
  "scripts": {
    "cmd": "",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "./scripts/prepare.sh",
    "dist": "yarn prepare && cd ./lib && yarn publish -f --ignore-scripts",
    "format": "prettier --write ./src/**/*.js"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-storyshots": "^3.3.15",
    "@storybook/cli": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "@types/jest": "^22.2.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-motion": "^0.0.25",
    "@types/react-test-renderer": "^16.0.1",
    "@types/storybook__react": "^3.0.7",
    "awesome-typescript-loader": "^4.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.3.3",
    "jest": "^22.4.2",
    "jest-enzyme": "^6.0.0",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.3.5",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.4.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
    "@fortawesome/fontawesome-free-solid": "^5.0.8",
    "@fortawesome/react-fontawesome": "^0.0.17",
    "classnames": "^2.2.5",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.38",
    "react-motion": "^0.5.2",
    "recompose": "^0.26.0"
  },
  "peerDependencies": {
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-jss": "^8.2.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "setupFiles": [
      "./jest/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/lib"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
